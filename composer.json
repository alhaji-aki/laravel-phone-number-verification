{
    "name": "alhaji-aki/laravel-phone-number-verification",
    "description": "A package to help verify phone numbers using otp tokens",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlhajiAki\\PhoneNumberVerification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\PhoneNumberVerification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\PhoneNumberVerification\\PhoneNumberVerificationServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "alhaji-aki/laravel-otp-token": "^1.1"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.3",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.5.1"
    },
    "scripts": {
        "analyse": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit",
        "format": "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
