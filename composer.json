{
    "name": "alhaji-aki/laravel-phone-number-verification",
    "description": "A package to help verify phone numbers using otp tokens",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlhajiAki\\PhoneNumberVerification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\PhoneNumberVerification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\PhoneNumberVerification\\PhoneNumberVerificationServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/contracts": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "alhaji-aki/laravel-otp-token": "^1.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "mockery/mockery": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^9.5.1"
    },
    "scripts": {
        "test": "phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
