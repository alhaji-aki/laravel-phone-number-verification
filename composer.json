{
    "name": "alhaji-aki/laravel-phone-number-verification",
    "description": "A package to help verify phone numbers using otp tokens",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Kudus",
            "email": "abdulkudus2922@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AlhajiAki\\PhoneNumberVerification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlhajiAki\\PhoneNumberVerification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "AlhajiAki\\PhoneNumberVerification\\PhoneNumberVerificationServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "alhaji-aki/laravel-otp-token": "^1.2"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "larastan/larastan": "^2.7.0|^3.0",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
